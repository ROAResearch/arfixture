{
    "name": "roaresearch/yii2-arfixture",
    "description": "Yii2 library to run fixtures the ActiveRecord Dao",
    "keywords": [
        "yii",
        "yii2",
        "tests",
        "testing",
        "dao",
        "activerecord",
        "ar",
        "fixture"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/ROAResearch/yii2-arfixture",
    "authors": [
        {
            "name": "Angel (Faryshta) Guevara",
            "email": "aguevara@tecnocen.com",
            "homepage": "https://github.com/Faryshta",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "roaresearch\\yii2\\arfixture\\": "src"
        }
    }
}
